    "type": "wordpress-plugin",
    "require": {
      "php": "^7.4 || ^8.0"
    },
    "autoload": {
      "psr-4": {
        "Ginopeterson\\MoxcarChatbot\\": "src/"
      }
    },
    "authors": [
      {
        "name": "Gino Peterson",
